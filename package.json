{
  "name": "@vpmedia/phixify-example",
  "version": "1.0.0",
  "description": "Example for the @vpmedia/phixify package",
  "author": "Andras Csizmadia <andras@vpmedia.hu> (www.vpmedia.hu)",
  "license": "MIT",
  "homepage": "https://github.com/vpmedia/phixify-example#readme",
  "bugs": {
    "url": "https://github.com/vpmedia/phixify-example/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vpmedia/phixify-example.git"
  },
  "type": "module",
  "main": "src/index.html",
  "scripts": {
    "start": "vite --open",
    "build": "vite build --emptyOutDir",
    "preview": "vite preview --open",
    "lint": "eslint \"**/*.{js,jsx}\"",
    "lint-fix": "eslint --fix \"**/*.{js,jsx}\"",
    "stylelint": "stylelint \"**/*.css\"",
    "typecheck": "tsc",
    "format": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "example1": "phixify all --project-dir project1 --config-file ./config/.phixify.multi_project_multi_bundle.json && phixify all --project-dir project2 --config-file ./config/.phixify.multi_project_multi_bundle.json",
    "example2": "phixify all --project-dir project1 --config-file ./config/.phixify.multi_project_single_bundle.json && phixify all --project-dir project2 --config-file ./config/.phixify.multi_project_single_bundle.json",
    "example3": "phixify all --config-file ./config/.phixify.single_project_multi_bundle.json",
    "example4": "phixify all --config-file ./config/.phixify.single_project_single_bundle.json",
    "example-all": "npm run example1 && npm run example2 && npm run example3 && npm run example4",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@pixi/sound": "^5.1.1",
    "fontfaceobserver": "^2.3.0",
    "phaser": "^3.60.0",
    "pixi.js-legacy": "^7.2.4",
    "spectorjs": "^0.9.27"
  },
  "devDependencies": {
    "@types/fontfaceobserver": "^2.1.0",
    "@vpmedia/phixify": "^1.4.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsdoc": "^43.0.7",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "stylelint": "^15.5.0",
    "stylelint-config-standard": "^33.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.0"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=18.0.0"
  }
}
