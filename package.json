{
  "name": "@vpmedia/phixify-example",
  "version": "1.0.0",
  "description": "Example for the @vpmedia/phixify package",
  "author": "Andras Csizmadia <andras@vpmedia.hu> (www.vpmedia.hu)",
  "license": "MIT",
  "homepage": "https://github.com/vpmedia/phixify-example#readme",
  "bugs": {
    "url": "https://github.com/vpmedia/phixify-example/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vpmedia/phixify-example.git"
  },
  "type": "module",
  "main": "src/index.html",
  "scripts": {
    "build": "vite build --emptyOutDir",
    "check": "pnpm lint && pnpm test && pnpm typecheck",
    "example-all": "pnpm run example1 && pnpm run example2 && pnpm run example3 && pnpm run example4",
    "example1": "phixify all --project-dir project1 --config-file ./config/.phixify.multi_project_multi_bundle.json && phixify all --project-dir project2 --config-file ./config/.phixify.multi_project_multi_bundle.json",
    "example2": "phixify all --project-dir project1 --config-file ./config/.phixify.multi_project_single_bundle.json && phixify all --project-dir project2 --config-file ./config/.phixify.multi_project_single_bundle.json",
    "example3": "phixify all --config-file ./config/.phixify.single_project_multi_bundle.json",
    "example4": "phixify all --config-file ./config/.phixify.single_project_single_bundle.json",
    "format": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json,css}\"",
    "lint": "eslint \"**/*.{js,jsx}\"",
    "preview": "vite preview --open",
    "start": "vite --open",
    "stylelint": "stylelint \"**/*.css\" --ignore-path .gitignore",
    "test": "vitest --coverage --pass-with-no-tests",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@pixi/sound": "^6.0.1",
    "fontfaceobserver": "^2.3.0",
    "phaser": "^3.90.0",
    "pixi.js": "^8.15.0",
    "spectorjs": "^0.9.30"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@eslint/js": "^9.39.2",
    "@types/fontfaceobserver": "^2.1.3",
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "@vpmedia/phixify": "^2.2.0",
    "eslint": "^9.39.2",
    "eslint-plugin-jsdoc": "^61.5.0",
    "eslint-plugin-oxlint": "^1.37.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^17.0.0",
    "jsdom": "^27.4.0",
    "oxlint": "^1.37.0",
    "oxlint-tsgolint": "^0.10.1",
    "prettier": "^3.7.4",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "vite": "8.0.0-beta.5",
    "vitest": "^4.0.16"
  },
  "browserslist": [
    "> 0.5%",
    "not dead",
    "not op_mini all",
    "iOS >= 14"
  ]
}
