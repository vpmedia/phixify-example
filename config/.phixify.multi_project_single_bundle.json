{
  "basePath": "./asset/multi_project_single_bundle/",
  "assetPath": "/asset/",
  "cache": false,
  "multiBundle": false,
  "engine": {
    "pixi_v7": true,
    "phaser_v3": true,
    "phixify": true
  },
  "output": {
    "pixi": "pixi_resource.json",
    "phaser": "phaser_resource.json",
    "phixify": "phixify_resource.json"
  },
  "dir": {
    "audioSprite": "audiosprite",
    "audioSpriteSource": "audiosprite-source",
    "image": "image",
    "sound": "sound",
    "spriteSheet": "spritesheet",
    "spriteSheetSource": "spritesheet-source"
  },
  "asset": {
    "image": [
      "avif",
      "webp"
    ],
    "sound": [
      "mp3",
      "ogg"
    ]
  },
  "manifest": {
    "image": [
      "avif",
      "webp",
      "png"
    ],
    "sound": [
      "mp3",
      "ogg",
      "wav"
    ]
  },
  "tool": {
    "image": "imageMagick",
    "sound": "sox",
    "spriteSheet": "texturePacker"
  },
  "cmd": {
    "texturePacker": {
      "path": "texturepacker",
      "opts": {
        "png": [
          "--format pixijs4",
          "--disable-rotation",
          "--force-squared",
          "--size-constraints POT",
          "--trim-sprite-names"
        ]
      }
    },
    "imageMagick": {
      "path": "magick",
      "opts": {
        "avif": [
          "-quality 90"
        ],
        "webp": [
          "-quality 90"
        ]
      }
    },
    "sox": {
      "path": "sox",
      "opts": {
        "mp3": [
          "-C 128.50"
        ],
        "ogg": [
          "-C 4"
        ]
      }
    },
    "ffmpeg": {
      "path": "ffmpeg",
      "opts": {
        "mp3": [],
        "ogg": []
      }
    },
    "ffprobe": {
      "path": "ffprobe"
    }
  }
}